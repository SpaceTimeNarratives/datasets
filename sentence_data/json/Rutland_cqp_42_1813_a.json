 Netherby.","file_id":"1813_a","plnames":[["Carlisle",78,86],["Netherby",119,127]],"geonouns":[],"pos_words":[["respectable",31,42]],"neg_words":[],"events":[],"dates":[["Wednesday",0,9]],"times":[["this morning",48,60]],"distances":[["12 miles",97,105]],"loc_advs":[],"spa_preps":[["out",70,73]],"normalised_sentiment":0.0833333333,"vader_sentiment":0.4,"roberta_sentiment":0.996094048},{"sent_id":7,"sentence":"Mr. Hayes accompanied me in the post chaise; Mr. Smith drove his own curricle; and Messrs. King and Carlyle travelled on horseback.","file_id":"1813_a","plnames":[],"geonouns":[["post",32,36]],"pos_words":[],"neg_words":[],"events":[],"dates":[],"times":[],"distances":[],"loc_advs":[],"spa_preps":[],"normalised_sentiment":0.0,"vader_sentiment":0.0,"roberta_sentiment":0.9954186678},{"sent_id":8,"sentence":"The morning was fine, but cold.","file_id":"1813_a","plnames":[],"geonouns":[],"pos_words":[["fine",16,20]],"neg_words":[["cold",26,30]],"events":[],"dates":[],"times":[["The morning",0,11]],"distances":[],"loc_advs":[],"spa_preps":[],"normalised_sentiment":0.0,"vader_sentiment":0.1,"roberta_sentiment":0.9972291589},{"sent_id":9,"sentence":"In three miles we passed through Long Town, a place of some size.","file_id":"1813_a","plnames":[["Long Town",33,42]],"geonouns":[],"pos_words":[],"neg_words":[],"events":[],"dates":[],"times":[],"distances":[["three miles",3,14]],"loc_advs":[],"spa_preps":[["through",25,32]],"normalised_sentiment":0.0,"vader_sentiment":0.0,"roberta_sentiment":-0.9294731617},{"sent_id":10,"sentence":"The road was frequently blocked up by droves of oxen travelling towards the South.","file_id":"1813_a","plnames":[],"geonouns":[["road",4,8]],"pos_words":[],"neg_words":[],"events":[],"dates":[],"times":[],"distances":[],"loc_advs":[],"spa_preps":[],"normalised_sentiment":0.0,"vader_sentiment":-0.27,"roberta_sentiment":-0.9993079901},{"sent_id":11,"sentence":"There was nothing striking in the approach to Carlisle.","file_id":"1813_a","plnames":